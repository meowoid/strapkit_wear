<html><script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script><script src="file:///android_asset/app.js"></script><body></body>
<script>
window.onload = function(){$.getJSON("http://www.reddit.com/r/pics/.json?jsonp=?", function(data) {
      window.items = [];
      for(var i=0;i<data.data.children.length; i++) {
          window.items.push({text: data.data.children[i].title});
      }

  })
}
</html>
